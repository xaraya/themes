/* Misc theme-specific styles */
div#header_content h1, div#header_content h2, div#header_content p, div#header_content ul, div#header_content li {
    margin-top: 0;
    margin-bottom: 0;
    list-style: none;
} 
div#header_content h1, div#header_content h2 {
    font-size: 12pt;
}
div#header_content ul, div#header_content li {
    margin: 0;
    padding: 0;
}
div#header_contentcore {
    margin: 0 20px 0 10px;
}
ul#navbar_core {
    margin: 0 20px 0 10px;
    padding: 0;
    height: 20px;
    z-index: 200;
}
ul#navbar_core li {
    float: left;
    width: 16.6%;
    text-align: left;
    list-style: none;
    font-size: 12pt;
    height: 100%;
}
ul#navbar_core a, div#contentmain h1 a {
    color: #f93;
    text-decoration: none;
    display: block;
    height: 100%;
}
div#contentmain h2 a, div#contentmain h3 a, div#contentmain h4 a {
    color: #f93;
    text-decoration: none;
}
ul#navbar_core a:hover, 
ul#navbar_core > li:hover > a, 
div#contentmain h1 a:hover, 
div#contentmain h2 a:hover, 
div#contentmain h3 a:hover, 
div#contentmain h4 a:hover {
    color: #FF5712;
}
ul#navbarcore a:hover {
    color: #FF5712;
}
ul#navbar_core div {
    width: 182px;
    padding: 9px 0 0 0;
    margin: 0;
    position: absolute;
    left: -999em;
}
ul#navbar_core li ul {
    width: 182px;
    margin: 0;
    padding: 0;
    background-image: url(/themes/xartropics/images/drop-bg.png);
    background-position: bottom;
    padding-bottom: 1em;
}
ul#navbar_core div li {
    z-index: 200 !important;
    height: auto;
}
ul#navbar_core li:hover div, li.sfhover div {
    z-index: 200 !important;
    left: auto !important;
}
ul#navbar_core li ul li {
    font-size: 10pt;
    margin: 0 1em .5em 1em;
    padding: 0;
    width: auto;
    float: none;
}
div#contentwrap {
    margin: 0 20px;
    clear: both;
}
div#contentmain {
    margin-left: 200px;
    color: #333;
    background: #fff url(/themes/xartropics/images/content-bgtl.png) 0 0 no-repeat;
}
div#contentmain_trcorner {
    height: 10px;
    font-size: 0;
    background: transparent url(/themes/xartropics/images/content-bgtr.png) top right no-repeat;
}
div#contentmain_inner {
    border-left: 1px solid #12ABE3;
    border-right: 1px solid #12ABE3;
}
div#contentmain_core {
    margin: 0 10px;
    padding: 1px 0;
}
div#contentmain li {
    list-style-image: url(/themes/xartropics/images/list-item.gif);
}
div#contentmain_core:after {
    content: '.';
    font-size: 0;
    clear: both;
    display: block;
    height: 0;
    visibility: hidden;
}
/* Hides from IE-mac \*/
* html div#contentmain_core {height: 1%;}
/* End hide from IE-mac */

div#contentmain_blcorner {
    height: 10px;
    font-size: 0;
    background: transparent url(/themes/xartropics/images/content-bgbl.png) 0 0 no-repeat;
}       
/*
* html div#contentmain_blcorner {
    margin-left: -3px;
}
*/
div#contentmain_brcorner {
    height: 10px;
    font-size: 0;
    background: transparent url(/themes/xartropics/images/content-bgbr.png) top right no-repeat;
}
div#contentleft {
    float: left;
    width: 180px;
}
/*
* html div#contentmain {
    margin-left: 197px !important;
}
*/
div#main_left {
    position: relative;
    margin-right: 310px;        
    padding-top: 1px;
}
div#main_right {
    position: relative;
    width: 280px;
    float: right;
    padding-top: 1px;
}
div#contentmain h3 {
    font-size: 12pt;
    color: #FF5712;
    border-bottom: 1px solid #FF5712 !important;
    margin-bottom: 0;
    margin-top: 0;
}
div#contentmain h4 {
    margin-bottom: 0;
    font-size: 11pt;
}
div.byline {
    color: #80AB1B;
    margin: 0;
    font-size: 9pt;
}
div#footerwrap {
    clear: both;
    background: #fff url(/themes/xartropics/images/footer_bg.png) bottom left repeat-x;
    height: 82px;
    margin-top: 20px;
}
div#footer_content {
    height: 70px;
    margin: 0 10px 0 20px;
    background-image: url(/themes/xartropics/images/foot-bgbr.png);
    background-position: bottom right;
    background-repeat: no-repeat;
}
div#footer_contentinner {
    background-image: url(/themes/xartropics/images/foot-bgtl.gif);
    background-position: top left;
    background-repeat: no-repeat;
    height: 50px;
}
div#footer_trcorner {
    height: 10px;
    background-image: url(/themes/xartropics/images/foot-bgtr.png);
    background-position: top right;
    background-repeat: no-repeat;
    font-size: 0;
}
div#footer_blcorner {
    height: 20px;
    background-image: url(/themes/xartropics/images/foot-bgbl.png);
    background-position: top left;
    background-repeat: no-repeat;
    font-size: 0;
}
div#footer_core {
    margin: 0 10px;
    text-align: center;
}

