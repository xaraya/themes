/* right block group classes */
div.xar-block-right {
    padding: 0;
    margin: 0 0 2em 0;
    width: 100%;
}
div.xar-block-head-right{
    background: #B3F5FA url(/themes/xartropics/images/block-head.gif) top center;
    background-repeat: no-repeat;
    margin: 0;
    padding: 6px 0 3px 0;
}
h3.xar-block-title-right {
    text-align: center;
    line-height: 1;
    font-size: 11pt;
    color: #1095C6;
    margin: 0px 10px;
}
div.xar-block-middle-right {
    height: 10px;
    background: #B3F5FA url(/themes/xartropics/images/block-wave.png) 0 0;
    border: 1px solid #ABE324;
    border-width: 0 1px;
    font-size: 0;
}
div.xar-block-body-right {
    background: #fff url(/themes/xartropics/images/block-body.png) top center;
    background-repeat: no-repeat;
    min-height: 125px;
}
div.xar-block-core-right {
    padding-top: 1px;
    margin: 0 10px;
}
.xar-block-foot-right {
}
div.xar-block-core-right ul {
    margin: 0;
    padding: 0;
    list-style: none;
}
div.xar-block-core-right li {
    margin-bottom: .5em;
}
div.xar-block-core-right *:first-child {
    margin-top: 0;
}

div#contentleft div.xar-block-right:first-child div.xar-block-head-right {
    background-image: url(/themes/xartropics/images/block-tophead.gif);
}



