<xar:template file="publinks" type="module" /><xar:categories-navigation layout="trails" showchildren="1" module="articles" itemtype="$ptid" itemid="$aid" cids="$cids" /><div class="article-view">    <div style="padding: 3px; background: #e0e0e0; border-bottom: solid 1px #008FD4;"><span class="xar-title">#$title#</span><xar:if condition="!empty($editurl)">                            (<a href="#$editurl#">#$edittitle#</a>)                        </xar:if></div>        <xar:if condition="!empty($categories) and count($categories) gt 0">            [ <xar:foreach in="$categories" value="$cat">                #$cat['cjoin']# <a href="#$cat['clink']#">#$cat['cname']#</a>            </xar:foreach> ]        </xar:if>    <img src="#xarTplGetThemeDir()#/images/download_icon.png" width="107" height="97" alt="download" style="float: left; margin: 10px;" />	<p>#$summary#</p>    <xar:if condition="empty($notes)"> <xar:elseif condition="xarModIsHooked('uploads','articles',$pubtypeid) and substr($notes,0,1) eq ';'" /><xar:comment>        If you hook the uploads module to articles, then file upload fields will contain        a comma-separated list of file ids, and you will need to call an API function to        retrieve the file information. For dynamic extra fields, you can use [fieldname]_output        directly to access the file information.    </xar:comment>        <xar:set name="notes_output">xarModAPIFunc('uploads','user','showoutput',array('value' => $notes, 'multiple' => true))</xar:set>        <xar:foreach in="$notes_output" key="$fileid" value="$fileinfo">            <p><span style="font-weight: bold;"><xar:mlstring>File Name</xar:mlstring>:</span> <a href="#$fileinfo['fileDownload']#">#$fileinfo['fileName']#</a></p>        </xar:foreach>    <xar:elseif condition="file_exists('var/uploads/'.$notes)" />        <p><a href="var/uploads/#$notes#" title="#$notes#">#$notes#</a></p>    <xar:else />        <p>#$notes#</p>    </xar:if></div><xar:if condition="!empty($prevart) || !empty($nextart)"><br /><table border="0" width="95%"><tr>    <td align="left"><xar:if condition="!empty($prevart)"><a href="#$prevart#">&lt; <xar:mlstring>prev</xar:mlstring> </a></xar:if>&nbsp;</td>    <td align="right">&nbsp;<xar:if condition="!empty($nextart)"> <a href="#$nextart#"><xar:mlstring>next</xar:mlstring> &gt;</a></xar:if></td></tr></table></xar:if><xar:if condition="!empty($hooks)"><div style="margin:60px 10px 10px 10px;"><xar:foreach in="$hooks" key="$hookmodule">#$hooks[$hookmodule]#</xar:foreach></div></xar:if><xar:template file="navtabs-user" type="module" />