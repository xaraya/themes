<xar:template file="publinks" type="module" />
<xar:set name="weblinkoverviewgroup"><xar:blockgroup name="weblink_overviewgroup" id="weblink_overviewgroup"/></xar:set>
<xar:if condition="$catid eq '7'">
    #$weblinkoverviewgroup#
	<xar:categories-navigation layout="trails" showchildren="1" module="articles" itemtype="6" catid="$catid" showcatcount="2" showempty="1" />
<xar:elseif condition="!empty($ptid) or !empty($catid)" />
	<xar:categories-navigation layout="trails" showchildren="1" module="articles" itemtype="6" catid="$catid" showcatcount="2" showempty="1" />
</xar:if>
<xar:if condition="!empty($author)">
	<h2>
		<xar:ml>
			<xar:mlstring>
				By Author : #(1) 
			</xar:mlstring>
			<xar:mlvar>
				#$author# 
			</xar:mlvar>
		</xar:ml>
	</h2>
	<br />
</xar:if>
<xar:if condition="!empty($numcols) and $number gt 0">
	<table border="0" cellpadding="0" cellspacing="0" width="100%">
		<tr>
			<xar:foreach in="$columns" key="$col">
				<td style="vertical-align: top;text-align: center;width: #$colwidth#%;">
					<xar:foreach in="$columns[$col]" key="$row">
						#$columns[$col][$row]# 
					</xar:foreach>
				</td>
			</xar:foreach>
		</tr>
	</table>
	<xar:elseif condition="!empty($number)" />
	<xar:foreach in="$columns" key="$col">
		<xar:foreach in="$columns[$col]" key="$row">
			#$columns[$col][$row]# 
		</xar:foreach>
	</xar:foreach>
</xar:if>
#$output# 
<xar:if condition="!empty($showcols)">
	<xar:if condition="$showcols eq 1">
		<a href="#xarModURL('articles','user','view',array('numcols' => 1))#">
			<xar:mlstring>
				single-column display 
			</xar:mlstring>
		</a>
		<xar:else />
		<a href="#xarModURL('articles','user','view')#">
			<xar:ml>
				<xar:mlstring>
					#(1)-column display 
				</xar:mlstring>
				<xar:mlvar>
					#$showcols# 
				</xar:mlvar>
			</xar:ml>
		</a>
	</xar:if>
</xar:if>
<!--
<xar:template file="alphapager" type="module" />
-->
<div style="text-align: center;">
	#$pager# 
</div>
<xar:if condition="!empty($showprevnext)">
	<xar:categories-navigation layout="prevnext" />
</xar:if>
<xar:template file="navtabs-user" type="module" />
